package Package2;

public class class2 {

	public static void test1() {
		
		System.out.println("This is static method");
		
	}
	
	public void test2() {
		
	System.out.println("This is non-static method");
	
		
	}
		for (int i=1; i<=5; i++) {
			
			for (int j=1; j<=i; j++) {
				
				System.out.print('*');
		}
			System.out.println();
	}

	for (int i=1; i<=4; i++) {
		
		for (int j=4; j>=i; j--) {
			
			System.out.print("*");
		}
		System.out.println();
	}
	
	
	
}
